{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "multi_board" "turret" "with_gun" "open_top" "automatic" "truck" "vision_lev02"  "muzzle_20mm_25mm_auto" "tractor" "tractor_medium"}
	{able "emit_on_attack"}
	{extension "ford_3ton_breda.mdl"}
	{extension "crew.ext"}
	{collider "truck"}
	{patherId "blitz36"}
	{targetSelector "tank_zenite"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))
	("gun" repair(30) thickness(15))
	("turret" repair(45))

	{volume "body2" {able {holed}}}
	{volume "turret3" {able {holed}}}

	("wood" args "bodyback1")
	("wood" args "bodyback2")
	("wood" args "bodyback3")
	("wood" args "bodyback4")
	("wood" args "bodyback5")
	("wood" args "bodyback6")
	("wood" args "bodyback7")
	("wood" args "bodyback8")
	("wood" args "bodyback9")
	("wood" args "bodyback10")
	
	{volume "turret1" {thickness 8}}
	{volume "turret2" {thickness 8}}
	{volume "turret3" {thickness 8}}

	{extender "inventory"
		{box
			{item "bullet_20x138b_auto fin ap" 192}		; 33 clips (12 rounds each)
			{item "bullet_20x138b_auto fin he" 204}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{weapon "20mm_itk35" filling "bullet_20x138b_auto fin he" 12}
			("abm_dymamic_aaa"
				zeroing(1.2)
				dispersion(0.15)
			)
		}
	}

	{mass 2870} ;//2540 + 330 gun
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(65) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(2.87) ;-------------------------------Weight in Tonnes.
			power(52) ;----------------------------------Horse Power.
			fuel(110) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(160) ;---------------------------------Range in Kilometers.
		)
	}
	{bone "gun_rot"
		{limits -10 85}
		{speed2 60}
	}
	{bone "turret"
		{speed2 60}
	}
}
