{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(0.9))
	(include "/properties/selection/vehicle.inc" scale(0.8))
	{props "vision_lev01"}
	{Extension "willysmb.mdl"}
	{able "emit_on_attack"}
	{PatherId "kubel"}
	{targetSelector "car_bullet"}
	{targetclass "car"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelleft1")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright1")
	("wheel" args "wheelright2")
	("glass" args "glass_front1")
	("glass" args "glass_front2")


	{extender "detail" {random 0} {detail1 0}}

	("crew_2_human")
	("crew_human" place(gunner) bone(gunner) door(emit3) animation(gunner))
	{placer
		{place "driver"		{group "driver"} {visor "driver"}}
		{place "commander"	{group "driver"} {visor "main"}}
		{place "gunner"		{group "driver"} {visor "main"} {turnoff {targetable 0}}}
	}

	{boarder
		{anm "driver"
			{forward	{begin	"board_willis_driver"} {end "seat_Willis_driver"}}
			{reverse 	{end	"board_willis_driver" -1}}
		}
		{anm "commander"
			{forward	{begin	"board_willis"} {end "seat_Willis_passenger"}}
			{reverse 	{end	"board_willis" -1}}
		}
		{anm "gunner"
			{forward {begin "board_LowTank_stand"} {end "pose_mgun_pz6"}}
			{reverse {end "emit_tank_1_hold" 1}}
		}
	}

	{sensor
		{visor "driver"
			{bone "visor2"}
			{vision "human"}
		}
		{visor "main"
			{bone "visor"}
			{vision "human"}
		}
	}
	
	{extender "inventory"
		{weapon "mgun" {mask "mountable"}}
		{Box
			{size 8 8}
			{item  "ammo 7x63" 1200}
			{item "browning_m19" filled {user "mgun"}}
		}
	}
	{Weaponry
		{place "mgun"
			{type "main"}
			{charger "gunner"}
			{LinkBone "mgun"}
			{LinkAnimation "off"}
			("abm_mgun")
		}
	}

	{mass 1100}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(105) ;----------------------------------Max speed.
			speedatmaxturn(12) ;-------------------------Max speed at max turn.
			reverse(21) ;--------------------------------Max speed in Reverse.
			weight(1.10) ;-------------------------------Weight in Tonnes.
			power(60) ;----------------------------------Horse Power.
			fuel(60) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(480) ;---------------------------------Range in Kilometers.
		)
	}
	{bone "mgun_turret"
		{speed2 8}
	}
}