{game_entity
	(include "/properties/tank.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(0.9))
	{props "light" "vision_lev03" "muzzle_75mm_76mm" "detect_tank_light"}
	{Extension "m24.mdl"}
	(include "m24.ext")
	{collider "tank.light"}
	{targetclass "tank_light"}
	{targetSelector "tank_light"}

		
	("crew_4_human")

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_LowArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_1"}}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	
	{extender "inventory"
		{box
			{item "dynamite"}
			{item "ammo 7x63" 4000}
			{item "bullet75_usa ap" 16}
            		{item "bullet75_usa he" 44}
            		{item "bullet75_usa apcbc" 11}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "75mm_m6" filling "bullet75_usa ap" 1}
			("abm_dymamic"
				zeroing(2.0)
				dispersion(0.15)
			)
		}
		{place "mgun1"
			{type "hull"}
			{weapon "browning_m19" filled}
			{foresight "foresight4"}
			{gunner "driver"}
			{charger "driver"}
			("abm_mgun")
			{basic}
		}
		{place "mgun2"
			{type "coaxial"}
			{weapon "browning_m19" filled}
			{foresight "foresight5"}
			{gunner "commander"}
			{charger "commander"}
			("abm_mgun")
			{basic}
		}
	}
	{mass 18400}
	{Chassis
		("tank_trace" fx("tracks_sm2") step(0.15) len(0.45))
		("mobility_tank"
			speed(56) ;----------Max speed when fully accelerated on road, km/h.
			reverse(22) ;--------Max speed in reverse on road, km/h.
			traverse(32) ;-------Track traverse, per second, in degrees.
			weight(18.4) ;--------Weight in Tonnes.
			power(220) ;---------Engine Power, in hp.
			track(3.5) ;---------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(420) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(160) ;---------Range in km, off-road.
		)
	}
	{bone "gun_rot"
		{speed2 10}
		{limits -10 15}
	}
	{bone "turret"
		{speed2 10}
	}
}
