{game_entity
	(include "/properties/airborne.ext")
	{props "wheeled" "vision_lev02_air"}
	{PatherID "bf110"}
	(include "/properties/shadow/airborn.inc" scale(2.2))
	(include "/properties/selection/airborne.inc" scale(2.25))
	(include "/properties/car_crew.ext")

	("fighter_tier2")

	{boarder
		{anm "driver"
			{forward {begin "board_plane_left"} {end "seat_driver_car"} {base "open"}}
			{reverse {end "board_plane_left" -1} {base "open" -1}}
		}
		{anm "gunner"
			{forward {begin "board_plane_left"} {end "seat_driver_car"} {base "open1"}}
			{reverse {end "board_plane_left" -1} {base "open1" -1}}
		}
	}

	("crew_human" place(driver) bone(driver) door(emit1) animation(driver) group(crew))
	("crew_human" place(commander) bone(commander) door(emit1) animation(driver) group(crew))

	{Placer
		{place "driver" {visor "main"} {turnOff {targetable}}}
		{place "gunner" {visor "rear"} {turnOff {targetable}}}
	}
	("crew_human" place(seat1) 	door(emit1) bone(seat01)	animation(driver) 	group(passengers))
	("crew_human" place(seat2) 	door(emit1) bone(seat02)	animation(driver)	group(passengers))
	("crew_human" place(seat3) 	door(emit1) bone(seat03)	animation(driver)	group(passengers))
	("crew_human" place(seat4) 	door(emit1) bone(seat04)	animation(driver)	group(passengers))
	("crew_human" place(seat5) 	door(emit1) bone(seat05)	animation(driver)	group(passengers))
	("crew_human" place(seat6) 	door(emit1) bone(seat06)	animation(driver)	group(passengers))
	("crew_human" place(seat7) 	door(emit1) bone(seat07)	animation(driver)	group(passengers))
	("crew_human" place(seat8) 	door(emit1) bone(seat08)	animation(driver)	group(passengers))
	("crew_human" place(seat9) 	door(emit1) bone(seat09)	animation(driver)	group(passengers))
	("crew_human" place(seat10) 	door(emit1) bone(seat10)	animation(driver)	group(passengers))
	("crew_human" place(seat11) 	door(emit1) bone(seat11)	animation(driver) 	group(passengers))
	("crew_human" place(seat12) 	door(emit1) bone(seat12)	animation(driver)	group(passengers))
	("double_engine")
	("wheel" args "wheelr")
	("wheel" args "wheell")
	("wheel" args "wheelback")

	{Chassis "wheel"
		("car_trace" fx("tracewheel05") step(0.07) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	}

	{Chassis "airborne"
		("flight_model"
			speed(480)  ;//---------Max speed (at sea level), km/h.
			climb_speed(11) ;//-----Climb rate at sea level, in m/s
			turn(250) ;//-----------Turn-and-burn radius in meters, tightest sustained turn
			turnMP(41) ;//----------Turn-and-burn radius in meters, tightest sustained turn in Multiplayer
			aerodynamics(1.55) ;//--Aerodynamic efficiency rating (0-3)
			weight(7600) ;//--------Weight in kg.
			power(2200) ;//---------Horse Power.
			fuel(1300) ;//----------Fuel capacity in Liters.
			range(1500) ;//---------Distance can travel with full fuel tank, in km.
			roll_rate(40) ;//-------Roll rate, in degrees per second.
			roll_angle(65) ;//------Maximum roll angle, in degrees.
		)
	}

	{mass 11800}

	{extender "inventory"
		{box
		}
	}
	{Extension "c-47.mdl"}
}